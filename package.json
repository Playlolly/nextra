{
  "name": "@playlolly/nextra-theme-docs",
  "version": "1.0.0",
  "description": "A Nextra documentation theme for the Playlolly website.",
  "main": "index.js",
  "module": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playlolly/nextra-theme-docs.git"
  },
  "bugs": {
    "url": "https://github.com/playlolly/nextra-theme-docs/issues"
  },
  "homepage": "https://github.com/playlolly/nextra-theme-docs#readme",
  "author": "Playlolly",
  "license": "MIT",
  "files": [
    "dist/*",
    "style.css"
  ],
  "types": "./dist/types.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js"
    },
    "./callout": {
      "import": "./dist/callout.js"
    },
    "./bleed": {
      "import": "./dist/bleed.js"
    },
    "./tabs": {
      "import": "./dist/components/tabs.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "build": "pnpm build:tailwind && pnpm build:layout",
    "build:layout": "node scripts/build.js",
    "build:tailwind": "cross-env NODE_ENV=production pnpm postcss src/styles.css -o style.css --verbose",
    "types": "tsc -p tsconfig.types.json",
    "dev:layout": "node scripts/dev.js",
    "dev:tailwind": "cross-env TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "prepublishOnly": "rm -rf dist && pnpm build",
    "test": "vitest --run"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/react": "^2.1.0",
    "@reach/skip-nav": "^0.16.0",
    "classnames": "^2.2.6",
    "flexsearch": "^0.7.21",
    "focus-visible": "^5.1.0",
    "github-slugger": "^1.4.0",
    "intersection-observer": "^0.12.0",
    "match-sorter": "^4.2.0",
    "next-themes": "^0.2.0-beta.2",
    "parse-git-url": "^1.0.1",
    "scroll-into-view-if-needed": "^2.2.29",
    "title": "^3.4.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.3",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "nextra": "2.0.0-alpha.54"
  }
}
